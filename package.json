{
  "name": "flockwave-web",
  "productName": "Flockwave",
  "version": "0.2.0",
  "description": "Web frontend for a Flockwave server",
  "main": "build/launcher.bundle.js",
  "scripts": {
    "bundle": "webpack -p --progress --config webpack/browser.config.js",
    "bundle:electron": "NODE_ENV=production webpack -p --progress --config webpack/electron.config.js && NODE_ENV=production webpack -p --progress --config webpack/launcher.config.js",
    "dist": "npm run bundle:electron && electron-builder -mwl",
    "doc": "esdoc -c esdoc.json",
    "lint": "eslint 'src/**'",
    "pack": "npm run bundle:electron && electron-builder --dir",
    "start": "webpack-dev-server --config webpack/browser.config.js --progress --colors",
    "start:electron": "webpack-dev-server --config webpack/electron.config.js --progress --colors",
    "start:public": "webpack-dev-server --config webpack/browser.config.js --progress --colors --host 0.0.0.0",
    "start:public:secure": "webpack-dev-server --config webpack/browser.config.js --progress --colors --https --host 0.0.0.0",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "webpack --watch"
  },
  "repository": {
    "type": "git",
