  "scripts": {
    "bundle": "rimraf build && cross-env NODE_ENV=production webpack --mode=production --progress --config webpack/dist.config.js",
    "dist": "node scripts/deploy.js -p",
    "dist:light": "node scripts/deploy.js -p --variant light",
    "lint": "xo",
    "start": "rimraf build && webpack serve --config webpack/browser.config.js --progress",
    "start:electron": "webpack serve --https --config webpack/electron.config.js --progress",
    "start:electron:light": "cross-env SKYBRUSH_VARIANT=light webpack serve --https --config webpack/electron.config.js --progress",
    "test": "npm run lint"
  },
