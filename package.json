  "description": "Web frontend for a Flockwave server",
  "main": "build/launcher.bundle.js",
  "scripts": {
    "bundle": "webpack --mode=production --progress --config webpack/browser.config.js",
    "dist": "node scripts/deploy.js -p",
    "doc": "esdoc -c esdoc.json",
    "lint": "eslint 'src/**'",
    "start": "webpack-dev-server --config webpack/browser.config.js --progress --colors",
    "start:electron": "webpack-dev-server --https --config webpack/electron.config.js --progress --colors",
    "start:public": "webpack-dev-server --config webpack/browser.config.js --progress --colors --host 0.0.0.0",
